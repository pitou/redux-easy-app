{
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "parserOptions": {
    "ecmaVersion": 7,
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "curly": [2, "all"],
    "eqeqeq": [2],
    "no-caller": [2],
    "no-new": [2],
    "no-param-reassign": [2],
    "no-self-compare": [2],
    "no-throw-literal": [2],
    "no-undef": [2],
    "no-unused-vars": [2, {"vars": "all", "args": "none"}],
    "no-use-before-define": [2],
    "no-var": [2],
    "strict": [2, "never"],
    "brace-style": [2, "1tbs"],
    "comma-spacing": [2, {"before": false, "after": true}],
    "eol-last": [2],
    //"indent": [2, 2],
    "key-spacing": [2, {"beforeColon": false, "afterColon": true}],
    "no-mixed-requires": [2],
    "no-mixed-spaces-and-tabs": [2],
    "no-trailing-spaces": [2],
    //"no-underscore-dangle": [2],
    "keyword-spacing": [2],
    "space-before-blocks": [2, "always"],
    "space-before-function-paren": [2, "never"],
    "space-in-parens": [2, "never"],
    "space-infix-ops": [2],
    "space-return-throw-case": [2],
    "space-unary-ops": [0],
    "comma-dangle": [0],
    "new-cap": [0],
    "no-alert": [0],
    "quotes": [0],
  }
}